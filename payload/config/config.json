{
  "debug": false,
  "work_dir": "/var/run/vanguard",

  "programs": {
    "direwolf": "/usr/local/bin/direwolf",
    "gpsd": "/usr/sbin/gpsd",
    "streamer": "/usr/bin/streamer"
  },

  "gps": {
    "device": "/dev/ttyO1",
    "gpsd_sock": "/var/run/gpsd.sock",
    "uart": "UART1"
  },

  "camera": {
    "device": "/dev/video0",
    "resolution": "1280x960",
    "quality": 90,
    "depth": 24,
    "interval": 15
  },

  "beacon": {
    "callsign": "N5JHH-2",
    "path": "WIDE2-1",

    "position_interval": 15,
    "telemetry_interval": 60,
    "beacon_interval": 300,
    "radio_uart": "UART5",
    "radio_device": "/dev/ttyO5",
    "radio_baudrate": 9600,
    "tnc_device": "/tmp/kisstnc",
    "tnc_baudrate": 1200
  },

  "temp": {
    "int_temp_pin": "P9_37",
    "ext_temp_pin": "P9_39",
    "interval": 5
  }
}
